import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner scnr = new Scanner(System.in);
		System.out.println("Digite el estrato:");
		float estrato= scnr.nextInt();
		System.out.println("Digite la cantidad de hombres:");
	 	float masculino= scnr.nextInt();
	 	System.out.println("Digite la cantidad de mujeres:");
	 	float femenino= scnr.nextInt();
	 	
	 	float sumatoria=masculino+femenino;
	 	
	 	 float promh= (masculino*100)/sumatoria;
	 	
	 	 float promm= (femenino*100)/sumatoria;
	 	 
	 	 System.out.println("el estrato es: "+estrato);
	 	 System.out.println("el porcentaje de hombre es: "+promh+"%");
	 	 System.out.println("el porcentaje de mujeres es: "+promm+"%");
	}
}
